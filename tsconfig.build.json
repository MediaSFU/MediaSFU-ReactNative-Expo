{
    "extends": "./tsconfig.json",
    "compilerOptions": {
      "noEmit": false,
      "outDir": "./dist",
      "declaration": true,
      "declarationDir": "./dist/types",
      "emitDeclarationOnly": false,
      "jsx": "react-native",
      "module": "ESNext",
      "target": "ES2020",
      "isolatedModules": true
    },
    "include": ["src","./main.tsx"],
    "exclude": ["node_modules", "build", "dist"]
  }
  